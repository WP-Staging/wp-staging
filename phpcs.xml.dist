<?xml version="1.0"?>
<ruleset name="WPSTAGING Coding Standards">

    <file>/var/www/single_tests/wp-content/plugins/wp-staging-pro</file>

    <arg name="extensions" value="php"/>

    <arg name="basepath" value="/"/>
    <arg name="colors"/>
    <arg name="parallel" value="8"/>
    <arg value="sp"/>

    <rule ref="PHPCompatibility"/>
    <config name="testVersion" value="5.5-"/>

    <exclude-pattern>*/vendor/composer/autoload_static.php</exclude-pattern>
</ruleset>
